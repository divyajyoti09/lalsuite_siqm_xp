prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: LALInference
Description: LAL Inference Library Support
Version: @VERSION@
Requires.private: gsl, lal >= @LAL_VERSION@, lalframe >= @LALFRAME_VERSION@, lalmetaio >= @LALMETAIO_VERSION@, lalsimulation >= @LALSIMULATION_VERSION@, lalburst >= @LALBURST_VERSION@, lalinspiral >= @LALINSPIRAL_VERSION@
Libs: -L${libdir} -llalinference
Cflags: -I${includedir} @OPENMP_CFLAGS@
